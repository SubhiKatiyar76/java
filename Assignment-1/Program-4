public class Main{
    public static void main(String[]arg)
    {
        int x=10;
        int y=5;
        System.out.println("Before swapping:");
        System.out.println("x="+x);
        System.out.println("y="+y);
        //Using addition and subtraction
        x=x+y; //x=15
        y=x-y; //y=10
        x=x-y;//x=5
        System.out.println("After swapping:");
        System.out.println("x="+x);
        System.out.println("y="+y);
    }
}
