import java.util.Scanner;

public class DigitChecker {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a angle a : ");
        int a=sc.nextInt();
        System.out.print("Enter a angle b : ");
        int b=sc.nextInt();
        System.out.print("Enter a angle c : ");
        int c=sc.nextInt();
        if((a+b+c)==180){
            System.out.println("Trigle can be formed");
        }else{
            System.out.println("Trigle can not be formed");
        }
        }
    }
