import java.util.*;
class Main {
    public static void main (String[]args){
        Scanner sc = new Scanner (System.in);
        System.out.println("Ente a number : ");
        int n=sc.nextInt();
        if(n<0){
            System.out.println("Factorial is not defined for negative number.");
        }else{
            long factorial = 1;
            for(int i=1;i<=n;i++){
                factorial *=i;
            }System.out.println(factorial);
        }
    }
}
