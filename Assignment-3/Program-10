public class FibonacciSeries {
    public static void main(String[] args) {
        int a = 0, b = 1;
        System.out.print(a + " " + b + " "); // Print the first two terms
        for (int c = a+b; c <= 100; c=a+b) {
            System.out.print(c + " ");
            a = b;
            b = c;
        }
    }
}
